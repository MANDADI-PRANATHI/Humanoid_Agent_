<?xml version="1.0"?>
<robot name="humanoid_10theta_stable">

  <!-- pelvis -->
  <link name="pelvis">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="12.0"/>
      <inertia ixx="0.6" ixy="0.0" ixz="0.0" iyy="0.6" iyz="0.0" izz="0.6"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.25" rpy="0 0 0"/>
      <geometry><box size="0.25 0.15 0.18"/></geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.25" rpy="0 0 0"/>
      <geometry><box size="0.25 0.15 0.18"/></geometry>
    </collision>
  </link>

  <!-- RIGHT LEG -->
  <link name="right_thigh">
    <inertial>
      <origin xyz="0 -0.05 -0.18"/>
      <mass value="4.0"/>
      <inertia ixx="0.04" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.015"/>
    </inertial>
    <visual><origin xyz="0 -0.05 -0.18"/><geometry><cylinder length="0.36" radius="0.06"/></geometry></visual>
    <collision><origin xyz="0 -0.05 -0.18"/><geometry><cylinder length="0.36" radius="0.06"/></geometry></collision>
  </link>
  <joint name="right_hip_pitch" type="revolute">
    <parent link="pelvis"/>
    <child link="right_thigh"/>
    <origin xyz="0 -0.08 -0.05"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.3" upper="2.3" effort="200" velocity="5.0"/>
  </joint>

  <link name="right_shank">
    <inertial>
      <origin xyz="0 -0.02 -0.17"/>
      <mass value="3.0"/>
      <inertia ixx="0.025" ixy="0" ixz="0" iyy="0.025" iyz="0" izz="0.012"/>
    </inertial>
    <visual><origin xyz="0 -0.02 -0.17"/><geometry><cylinder length="0.34" radius="0.05"/></geometry></visual>
    <collision><origin xyz="0 -0.02 -0.17"/><geometry><cylinder length="0.34" radius="0.05"/></geometry></collision>
  </link>
  <joint name="right_knee" type="revolute">
    <parent link="right_thigh"/>
    <child link="right_shank"/>
    <origin xyz="0 0 -0.36"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.5" upper="2.0" effort="150" velocity="5.0"/>
  </joint>

  <link name="right_foot">
    <inertial>
      <origin xyz="0 0 -0.05"/>
      <mass value="1.0"/>
      <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
    </inertial>
    <visual><origin xyz="0 0 -0.05"/><geometry><box size="0.22 0.08 0.03"/></geometry></visual>
    <collision><origin xyz="0 0 -0.05"/><geometry><box size="0.22 0.08 0.03"/></geometry></collision>
  </link>
  <joint name="right_ankle" type="revolute">
    <parent link="right_shank"/>
    <child link="right_foot"/>
    <origin xyz="0 0 -0.34"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.8" effort="80" velocity="8.0"/>
  </joint>

  <!-- LEFT LEG -->
  <link name="left_thigh">
    <inertial>
      <origin xyz="0 0.05 -0.18"/>
      <mass value="4.0"/>
      <inertia ixx="0.04" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.015"/>
    </inertial>
    <visual><origin xyz="0 0.05 -0.18"/><geometry><cylinder length="0.36" radius="0.06"/></geometry></visual>
    <collision><origin xyz="0 0.05 -0.18"/><geometry><cylinder length="0.36" radius="0.06"/></geometry></collision>
  </link>
  <joint name="left_hip_pitch" type="revolute">
