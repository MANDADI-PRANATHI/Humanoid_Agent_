    <visual><origin xyz="0 0.06 0.0"/><geometry><cylinder length="0.26" radius="0.04"/></geometry></visual>
    <collision><origin xyz="0 0.06 0.0"/><geometry><cylinder length="0.26" radius="0.04"/></geometry></collision>
  </link>
  <joint name="left_shoulder_pitch" type="revolute">
    <parent link="torso"/>
