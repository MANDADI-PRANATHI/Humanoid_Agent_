  </joint>

  <link name="right_forearm">
    <inertial><origin xyz="0 -0.02 -0.12"/><mass value="1.5"/>
      <inertia ixx="0.015" ixy="0" ixz="0" iyy="0.015" iyz="0" izz="0.007"/>
