  <link name="left_forearm">
    <inertial><origin xyz="0 0.02 -0.12"/><mass value="1.5"/>
      <inertia ixx="0.015" ixy="0" ixz="0" iyy="0.015" iyz="0" izz="0.007"/>
    </inertial>
    <visual><origin xyz="0 0.02 -0.12"/><geometry><cylinder length="0.24" radius="0.035"/></geometry></visual>
    <collision><origin xyz="0 0.02 -0.12"/><geometry><cylinder length="0.24" radius="0.035"/></geometry></collision>
  </link>
  <joint name="left_elbow" type="revolute">
    <parent link="left_upperarm"/>
    <child link="left_forearm"/>
    <origin xyz="0 0 -0.26"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.5" upper="3" effort="50" velocity="6.0"/>
  </joint>

  <link name="left_hand">
    <inertial><origin xyz="0 0 0"/><mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
    <visual><origin xyz="0 0 0"/><geometry><box size="0.06 0.02 0.1"/></geometry></visual>
