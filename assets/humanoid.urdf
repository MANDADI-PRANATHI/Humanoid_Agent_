  <!-- head -->
  <link name="head">
    <inertial><origin xyz="0 0 0.09"/><mass value="1.5"/><inertia ixx="0.015" ixy="0" ixz="0" iyy="0.015" iyz="0" izz="0.015"/></inertial>
    <visual><origin xyz="0 0 0.09"/><geometry><sphere radius="0.08"/></geometry></visual>
    <collision><origin xyz="0 0 0.09"/><geometry><sphere radius="0.08"/></geometry></collision>
  </link>
  <joint name="neck" type="fixed">
    <parent link="torso"/>
    <child link="head"/>
    <origin xyz="0 0 0.27"/>
  </joint>

</robot>
