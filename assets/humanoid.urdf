  <link name="right_hand">
    <inertial><origin xyz="0 0 0"/><mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
    <visual><origin xyz="0 0 0"/><geometry><box size="0.06 0.02 0.1"/></geometry></visual>
    <collision><origin xyz="0 0 0"/><geometry><box size="0.06 0.02 0.1"/></geometry></collision>
  </link>
  <joint name="right_wrist" type="revolute">
    <parent link="right_forearm"/>
    <child link="right_hand"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="5"/>
  </joint>

  <!-- LEFT ARM -->
  <link name="left_upperarm">
    <inertial><origin xyz="0 0.06 0.0"/><mass value="2.0"/>
      <inertia ixx="0.025" ixy="0" ixz="0" iyy="0.025" iyz="0" izz="0.015"/>
