  <link name="left_shank">
    <inertial>
      <origin xyz="0 0.02 -0.17"/>
      <mass value="3.0"/>
      <inertia ixx="0.025" ixy="0" ixz="0" iyy="0.025" iyz="0" izz="0.012"/>
    </inertial>
    <visual><origin xyz="0 0.02 -0.17"/><geometry><cylinder length="0.34" radius="0.05"/></geometry></visual>
    <collision><origin xyz="0 0.02 -0.17"/><geometry><cylinder length="0.34" radius="0.05"/></geometry></collision>
  </link>
  <joint name="left_knee" type="revolute">
    <parent link="left_thigh"/>
    <child link="left_shank"/>
    <origin xyz="0 0 -0.36"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.5" upper="2.0" effort="150" velocity="5.0"/>
  </joint>

  <link name="left_foot">
    <inertial>
      <origin xyz="0 0 -0.05"/>
      <mass value="1.0"/>
      <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
    </inertial>
    <visual><origin xyz="0 0 -0.05"/><geometry><box size="0.22 0.08 0.03"/></geometry></visual>
    <collision><origin xyz="0 0 -0.05"/><geometry><box size="0.22 0.08 0.03"/></geometry></collision>
  </link>
  <joint name="left_ankle" type="revolute">
    <parent link="left_shank"/>
    <child link="left_foot"/>
    <origin xyz="0 0 -0.34"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.8" effort="80" velocity="8.0"/>
  </joint>

  <!-- torso -->
  <link name="torso">
    <inertial>
      <origin xyz="0 0 0.12"/>
      <mass value="10.0"/>
      <inertia ixx="0.25" ixy="0" ixz="0" iyy="0.25" iyz="0" izz="0.25"/>
    </inertial>
    <visual><origin xyz="0 0 0.12"/><geometry><box size="0.28 0.18 0.24"/></geometry></visual>
    <collision><origin xyz="0 0 0.12"/><geometry><box size="0.28 0.18 0.24"/></geometry></collision>
  </link>
  <joint name="pelvis_to_torso" type="fixed">
    <parent link="pelvis"/>
    <child link="torso"/>
    <origin xyz="0 0 0.10"/>
  </joint>

  <!-- RIGHT ARM -->
  <link name="right_upperarm">
